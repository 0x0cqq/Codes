{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "runner": "terminal",





    "tasks": [
        {
            "label": "g++",
            "type": "shell",
            "command": "g++-8",
            "windows": {
                "command": "g++"
            },
            "args": [
                "-std=c++11","${file}",
                "-o","${file}.out","-g"
            ],
        }
    ]
}
